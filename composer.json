{
	"name": "va5ja/rdn-console",
	"description": "Zend Framework 2 Symfony Console module",
	"keywords": ["zf2", "zend", "sf2", "symfony", "console"],
	"license": "MIT",
	"authors": [
		{
			"name": "radnan",
			"email": "rafiadnan47@gmail.com"
		}
	],
	"require": {
		"radnan/rdn-factory": "1.*",
		"symfony/console": "4.2.*",
		"zendframework/zend-mvc": "2.*",
		"zendframework/zend-servicemanager": "2.*",
		"zendframework/zend-stdlib": "2.*"
	},
	"autoload": {
		"psr-0": {
			"RdnConsole\\": "src/"
		}
	},
	"bin": ["bin/console"],
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "4.2.*"
		}
	}
}
